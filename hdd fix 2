sudo diskutil unmount disk2

sudo diskutil unmount disk2s1
sudo diskutil mountDisk disk2s2


sudo diskutil unmount /dev/disk2s2
sudo diskutil mount /dev/disk2s2

sudo diskutil unmountDisk /dev/disk2
sudo diskutil mountDisk /dev/disk2
sudo diskutil unmountDisk disk2s1
sudo diskutil unmountDisk disk2s2


sudo diskutil mount disk2
sudo diskutil mount disk2s1
sudo diskutil mount disk2s2

sudo diskutil mountDisk disk2
sudo diskutil mountDisk disk2s1
sudo diskutil mountDisk disk2s2



sudo diskutil unmount disk2
sudo diskutil unmountDisk /dev/disk2
sudo diskutil unmount disk2s1
sudo diskutil unmountDisk disk2s2