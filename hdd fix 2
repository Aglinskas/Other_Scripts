sudo diskutil unmount disk1


sudo diskutil unmount disk1s1
sudo diskutil mountDisk disk1s2


sudo diskutil unmount /dev/disk1s2
sudo diskutil mount /dev/disk1s2

sudo diskutil unmountDisk /dev/disk1
sudo diskutil mountDisk /dev/disk1
sudo diskutil unmountDisk disk1s1
sudo diskutil unmountDisk disk1s2


sudo diskutil mount disk1
sudo diskutil mount disk1s1
sudo diskutil mount disk1s2

sudo diskutil mountDisk disk1
sudo diskutil mountDisk disk1s1
sudo diskutil mountDisk disk1s2